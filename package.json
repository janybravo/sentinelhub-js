  "version": "1.0.0",
  "main": "dist/sentinelHub.cjs.js",
  "module": "dist/sentinelHub.esm.js",
  "browser": "dist/sentinelHub.umd.js",
  "dependencies": {
    "@turf/helpers": "^6.1.4",
    "@types/jest": "^25.1.2",
    "@types/xml2js": "^0.4.4",
    "axios": "^0.18.1",
