  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint --max-warnings 0 --ext .ts,js src/",
    "prettier": "prettier --write \"{src,example}/**/*.{ts,js}\"",
